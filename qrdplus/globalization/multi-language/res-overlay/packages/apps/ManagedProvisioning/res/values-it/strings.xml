  <string name="company_controls_device">"La tua organizzazione controllerà il dispositivo e lo manterrà protetto."</string>
  <!-- String shown above the icon of the app that will control the work profile after it is set up. [CHAR LIMIT=NONE]-->
  <string name="the_following_is_your_mdm">"La seguente app dovrà accedere a questo profilo:"</string>
