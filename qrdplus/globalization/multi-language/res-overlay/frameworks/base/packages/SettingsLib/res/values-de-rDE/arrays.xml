        <item>"Authentifizierung mit <xliff:g id="NETWORK_NAME">%1$s</xliff:g>..."</item>
        <!-- Status message of Wi-Fi when it is obtaining IP address from a network. -->
        <item>"IP-Adresse wird von <xliff:g id="NETWORK_NAME">%1$s</xliff:g> abgerufen..."</item>
