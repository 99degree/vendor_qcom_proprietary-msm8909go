        <item>"Verbindung mit <xliff:g id="NETWORK_NAME">%1$s</xliff:g> wird getrennt..."</item>
        <!-- Status message of Wi-Fi when it is disconnected. -->
        <item>"Nicht verbunden"</item>
        <!-- Status message of Wi-Fi when it is a failure. -->
        <item>"Nicht erfolgreich"</item>
