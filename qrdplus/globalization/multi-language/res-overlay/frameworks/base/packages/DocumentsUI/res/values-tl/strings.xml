    <string name="menu_advanced_hide" product="nosdcard">"Itago internal storage"</string>
    <!-- Menu item that hides SD cards built into the device [CHAR LIMIT=24] -->
    <string name="menu_advanced_hide" product="default">"Itago ang SD card"</string>
    <!-- Menu item that reveals the sizes of displayed files [CHAR LIMIT=24] -->
    <string name="menu_file_size_show">"Ipakita ang laki ng file"</string>
