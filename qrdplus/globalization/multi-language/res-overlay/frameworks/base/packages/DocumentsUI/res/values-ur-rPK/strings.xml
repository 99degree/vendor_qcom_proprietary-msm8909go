    <string name="create_error">"فولڈر بنانے میں ناکام ہو گیا"</string>
    <!-- Error message shown when querying for a list of documents failed [CHAR LIMIT=48] -->
    <string name="query_error">"دستاویزات استفسار کرنے میں ناکام"</string>
