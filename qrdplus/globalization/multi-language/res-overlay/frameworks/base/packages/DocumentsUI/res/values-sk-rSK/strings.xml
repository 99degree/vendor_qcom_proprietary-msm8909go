    <string name="menu_advanced_show" product="default">"Zobraziť kartu SD"</string>
    <!-- Menu item that hides internal storage built into the device [CHAR LIMIT=24] -->
    <string name="menu_advanced_hide" product="nosdcard">"Skryť interné úložisko"</string>
    <!-- Menu item that hides SD cards built into the device [CHAR LIMIT=24] -->
    <string name="menu_advanced_hide" product="default">"Skryť kartu SD"</string>
    <!-- Menu item that reveals the sizes of displayed files [CHAR LIMIT=24] -->
    <string name="menu_file_size_show">"Zobraziť veľkosť súboru"</string>
    <!-- Menu item that hides the sizes of displayed files [CHAR LIMIT=24] -->
    <string name="menu_file_size_hide">"Skryť veľkosť súboru"</string>
