    <string name="create_error">"Priečinok sa nepodarilo vytvoriť"</string>
    <!-- Error message shown when querying for a list of documents failed [CHAR LIMIT=48] -->
    <string name="query_error">"Zoznam dokumentov sa nepodarilo načítať"</string>
