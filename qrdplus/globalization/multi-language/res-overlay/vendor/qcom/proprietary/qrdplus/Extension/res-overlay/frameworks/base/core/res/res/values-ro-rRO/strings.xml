<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="permlab_accessWifiState">"vizualizați conexiunile WLAN"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_accessWifiState">"Permite aplicației să vizualizeze informațiile despre rețelistica WLAN, precum dacă WLAN este activată și numele dispozitivelor WLAN conectate."</string>
    <string name="permlab_changeWifiState">"conectați-vă și deconectați-vă de la WLAN"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_changeWifiState">"Permite aplicației să se conecteze și să se deconecteze la/de la punctele de acces WLAN și să efectueze modificări ale configurației dispozitivului pentru rețelele WLAN."</string>
    <string name="permlab_changeWifiMulticastState">"permiteți recepţionarea de difuzare multiplă WLAN"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_changeWifiMulticastState" product="tablet">"Permite aplicației să primească pachetele trimise tuturor dispozitivelor, nu numai tabletei, pe o rețea WLAN utilizând adrese de difuzare multiplă.  Această funcţie utilizează mai multă energie decât modul fără difuzare multiplă."</string>
    <string name="permdesc_changeWifiMulticastState" product="default">"Permite aplicației să primească pachetele trimise tuturor dispozitivelor, nu numai telefonului, pe o rețea WLAN utilizând adrese de difuzare multiplă.  Această funcție utilizează mai multă energie decât modul fără difuzare multiplă."</string>
    <string name="wifi_available_sign_in">"Conectați-vă la rețeaua WLAN"</string>
    <string name="wifi_watchdog_network_disabled">"Nu s-a putut conecta la WLAN"</string>
    <string name="wifi_p2p_dialog_title">"WLAN Direct"</string>
    <string name="wifi_p2p_turnon_message">"Porniți WLAN Direct. Acest lucru va dezactiva clientul/hotspotul WLAN."</string>
    <string name="wifi_p2p_failed_message">"WLAN Direct nu a putut porni."</string>
    <string name="wifi_p2p_enabled_notification_title">"WLAN Direct este pornit"</string>
    <string name="wifi_p2p_frequency_conflict_message" product="tablet">"Tableta se va deconecta temporar de la rețeaua WLAN cât timp este conectată la <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <string name="wifi_p2p_frequency_conflict_message" product="default">"Telefonul se va deconecta temporar de la rețeaua WLAN cât timp este conectată la <xliff:g id="device_name">%1$s</xliff:g>"</string>
